{
    "name": "oig-supervisor-api",
    "version": "1.0.0",
    "description": "oig app",
    "main": "src/index.ts",
    "scripts": {
        "start": "tsc && npm run migrate && npm run seed && nodemon dist/src/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "NODE_OPTIONS='--max-old-space-size=4096' nodemon src/index.ts",
        "local": "npm run migrate && npm run seed && nodemon src/index.ts",
        "build": "tsc -p .",
        "migrate": "npx sequelize-cli db:migrate",
        "seed": "npx sequelize-cli db:seed:all --debug",
        "undo-all-seed": "npx sequelize-cli db:seed:undo:all --debug",
        "undo-seed": "npx sequelize-cli db:seed:undo --debug",
        "undo-migrate": "npx sequelize-cli db:migrate:undo --debug",
        "undo-all-migrate": "npx sequelize-cli db:migrate:undo:all --debug"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.162.0",
        "@sentry/integrations": "^7.12.1",
        "@sentry/node": "^7.12.1",
        "@sentry/tracing": "^7.12.1",
        "@types/async": "^3.2.15",
        "@types/morgan": "^1.9.3",
        "aws-sdk": "^2.1217.0",
        "axios": "^1.3.2",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^9.0.0",
        "mime-types": "^2.1.35",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "run": "^1.4.0",
        "uuid": "^9.0.0",
        "winston": "^3.8.1",
        "winston-cloudwatch": "^6.1.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/mime-types": "^2.1.1",
        "@types/mysql": "^2.15.21",
        "@types/node": "^18.7.11",
        "express-async-errors": "^3.1.1",
        "http-terminator": "^3.2.0",
        "mysql2": "^2.3.3",
        "nodemon": "^2.0.19",
        "sequelize": "^6.21.4",
        "sequelize-cli": "^6.5.2",
        "sequelize-typescript": "^2.1.3",
        "swagger-jsdoc": "^6.2.5",
        "swagger-ui-express": "^4.5.0",
        "ts-md5": "^1.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
